{
    "menus": {
        "error": {
            "text": "Это меню ещё не создано",
            "buttons":{"return":"main"}
        },

        "error_command": {
            "text": "Команда не найдена",
            "buttons":{"return":"main"}
        },

        "main": {
            "text": "Главное меню",
            "buttons":{
                "profile:admin-10:20":"Профиль",
                "input":"Ввод",
                "admin":"admin"
            }
        },

        "input": {
            "text": "Введите текст",
            "return":"main",
            "handler": {
                "function":"input_text",
                "menu":"test"
            }
        },

        "test":{
            "text":"Вы ввели `{input_text}`",
            "return":"input"
        },

        "admin":{
            "text":"Панель администратора",
            "buttons":{
                "newsletter":"Рассылка"
            },
            "return":"main"
        },

        "newsletter":{
            "text":"Введите текст, для рассылки",
            "return":"admin",
            "handler": {
                "function":"newsletter",
                "menu":"newsletter_send"
            }
        },

        "newsletter_send":{
            "text":"Рассылка отправлена",
            "return":"admin",
            "send":{
                "menu":"newsletter_test",
                "recipient":"all"
            }
        },

        "newsletter_test":{
            "function":"newsletter",
            "buttons":{
                "notification":"notification"
            }
        },

        "profile":{
            "text":"TTA id: {tta_id}\nTelegram id: {telegram_id}\nЛинк: @{username}\nВремя регистрации\n{time_registration}\nРоль: {tta_role}",
            "buttons":{
                "return":"main"
            }
        }
    },

    "commands": {
        "start": {
            "menu": "main",
            "text": "Перезапуск бота"
        }
    },
    "var_buttons": {
        "return": "< Назад",
        "admin": {
            "text":"Администратор",
            "role":"admin"
        },
        "notification": "Прочитано"
    }
}