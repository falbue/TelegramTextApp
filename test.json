{
    "menus": {
        "error": {
            "text": "Это меню ещё не создано",
            "buttons":{"return":"main"}
        },

        "error_command": {
            "text": "Команда не найдена",
            "buttons":{"return":"main"}
        },

        "main": {
            "text": "Главное меню",
            "buttons":{
                "profile":"Профиль",
                "input":"Ввод",
                "query":"Успешно",
                "admin":"admin"
            }
        },

        "input": {
            "text": "Введите текст",
            "return":"main",
            "handler": {
                "function":"input_text",
                "menu":"test"
            }

        },

        "query": {
            "query":{
                "text":"Ахуенно",
                "show_alert":false
            }
        },

        "test":{
            "text":"Вы ввели `{input_text}`",
            "return":"input"
        },

        "admin":{
            "text":"Панель администратора",
            "buttons":{
                "start_work":"Работаем",
                "close_work":"Завершение работы",
                "newsletter":"Рассылка"
            },
            "return":"main"
        },

        "start_work":{
            "query":{
                "text":"Рассылка отправлена!",
                "show_alert":false
            },
            "send":{
                "text":"Админ на месте",
                "recipient":"admin"
            }
        },

        "newsletter":{
            "text":"Введите текст, для рассылки",
            "return":"admin",
            "handler": {
                "function":"newsletter",
                "menu":"newsletter_send"
            }
        },

        "newsletter_send":{
            "text":"Рассылка отправлена",
            "return":"admin",
            "send":{
                "menu":"newsletter_test",
                "recipient":"all"
            }
        },

        "newsletter_test":{
            "text":"*Рассылка:*\n{input_text}",
            "buttons":{
                "notification":"notification"
            }
        },

        "profile":{
            "text":"*TTA id:* `{tta_id}`\n*Telegram id:* `{telegram_id}`\n*Линк:* @{username}\n*Время регистрации:*\n`{time_registration}`\n*Роль:* _{tta_role}_",
            "buttons":{
                "return":"main"
            },
            "loading":"Загрузка..."
        }
    },

    "commands": {
        "start": {
            "menu": "main",
            "text": "Перезапуск бота"
        }
    },
    "var_buttons": {
        "return": "‹ Назад",
        "admin": {
            "text":"Администратор",
            "role":"admin"
        },
        "notification": "Прочитано"
    }
